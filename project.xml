<?xml version="1.0" encoding="UTF-8"?>
<buildster directory="../build">
	<project directory="../project">
		<dependencies>
			<dependency>
				<label>vfspp</label>
				<subpath>build</subpath>
				<remote>
					<url>https://github.com/PierceLBrooks/vfspp.git</url>
					<git_repo>
						<branch>piercelbrooks</branch>
						<credentials>
							<username>username</username>
							<password>password</password>
						</credentials>
					</git_repo>
				</remote>
				<build>
					<arguments>
						<argument>
							<switch id="BUILDSTER_OS">
								<case check="Windows">-DPLATFORM=Windows</case>
								<case check="Linux">-DPLATFORM=Linux</case>
								<case check="Darwin">-DPLATFORM=macOS</case>
								<default><quit></quit></default>
							</switch>
						</argument>
					</arguments>
					<cmake>
						<generator>
							<switch id="BUILDSTER_OS">
								<case check="Windows">MinGW Makefiles</case>
								<case check="Linux">Unix Makefiles</case>
								<case check="Darwin">Unix Makefiles</case>
								<default><quit></quit></default>
							</switch>
						</generator>
						<source>..</source>
					</cmake>
				</build>
				<exports>
					<export type="headers">
						<key>VFSPP_INCLUDE_PATH</key>
						<value><install></install>/include</value>
					</export>
					<export type="libraries">
						<key>VFSPP_LIBRARIES</key>
						<value><install></install>/lib</value>
					</export>
				</exports>
				<imports></imports>
			</dependency>
			<dependency>
				<label>openssl</label>
				<subpath>build</subpath>
				<remote>
					<url>https://github.com/PierceLBrooks/openssl-cmake.git</url>
					<git_repo>
						<branch>1.1.1g-20200510</branch>
						<credentials>
							<username>username</username>
							<password>password</password>
						</credentials>
					</git_repo>
				</remote>
				<build>
					<arguments></arguments>
					<cmake>
						<generator>
							<switch id="BUILDSTER_OS">
								<case check="Windows">MinGW Makefiles</case>
								<case check="Linux">Unix Makefiles</case>
								<case check="Darwin">Unix Makefiles</case>
								<default><quit></quit></default>
							</switch>
						</generator>
						<source>..</source>
					</cmake>
				</build>
				<exports>
					<export type="headers">
						<key>OPENSSL_INCLUDE_PATH</key>
						<value><install></install>/include</value>
					</export>
					<export type="libraries">
						<key>OPENSSL_LIBRARIES</key>
						<value><install></install>/lib</value>
					</export>
				</exports>
				<imports></imports>
			</dependency>
			<dependency>
				<label>libssh2</label>
				<subpath>build</subpath>
				<remote>
					<url>https://github.com/PierceLBrooks/libssh2.git</url>
					<git_repo>
						<branch>piercelbrooks</branch>
						<credentials>
							<username>username</username>
							<password>password</password>
						</credentials>
					</git_repo>
				</remote>
				<build>
					<arguments></arguments>
					<cmake>
						<generator>
							<switch id="BUILDSTER_OS">
								<case check="Windows">MinGW Makefiles</case>
								<case check="Linux">Unix Makefiles</case>
								<case check="Darwin">Unix Makefiles</case>
								<default><quit></quit></default>
							</switch>
						</generator>
						<source>..</source>
					</cmake>
				</build>
				<exports>
					<export type="headers">
						<key>LIBSSH_INCLUDE_PATH</key>
						<value><install></install>/include</value>
					</export>
					<export type="libraries">
						<key>LIBSSH_LIBRARIES</key>
						<value><install></install>/lib</value>
					</export>
				</exports>
				<imports>
					<import>openssl</import>
				</imports>
			</dependency>
			<dependency>
				<label>libgit2</label>
				<subpath>build</subpath>
				<remote>
					<url>https://github.com/PierceLBrooks/libgit2.git</url>
					<git_repo>
						<branch>piercelbrooks</branch>
						<credentials>
							<username>username</username>
							<password>password</password>
						</credentials>
					</git_repo>
				</remote>
				<build>
					<arguments></arguments>
					<cmake>
						<generator>
							<switch id="BUILDSTER_OS">
								<case check="Windows">MinGW Makefiles</case>
								<case check="Linux">Unix Makefiles</case>
								<case check="Darwin">Unix Makefiles</case>
								<default><quit></quit></default>
							</switch>
						</generator>
						<source>..</source>
					</cmake>
				</build>
				<exports>
					<export type="headers">
						<key>LIBGIT_INCLUDE_PATH</key>
						<value><install></install>/include</value>
					</export>
					<export type="libraries">
						<key>LIBGIT_LIBRARIES</key>
						<value><install></install>/lib</value>
					</export>
				</exports>
				<imports>
					<import>openssl</import>
					<import>llibssh2</import>
				</imports>
			</dependency>
		</dependencies>
		<targets>
			<target type="library">
				<label>memgit</label>
				<definitions></definitions>
				<links></links>
				<exports></exports>
				<imports>
					<import>libgit2</import>
					<import>libssh2</import>
					<import>openssl</import>
					<import>vfspp</import>
				</imports>
			</target>
		</targets>
	</project>
</buildster>
